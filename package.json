{
  "name": "serveportal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db_update": "npx prisma db push",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "translations:sort": "sort-json src/messages/*.json -i",
    "translations:format": "prettier --write src/messages",
    "translations:extract": "i18next 'src/**/*.{ts,tsx}' && pnpm run translations:sort && pnpm run translations:format",
    "translations:check": "i18next 'src/**/*.{ts,tsx}' --fail-on-update && pnpm run translations:sort && pnpm run translations:format"
  },
  "dependencies": {
    "@maptiler/sdk": "3.8.0",
    "@prisma/client": "6.17.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "0.5.6",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "i18next-parser": "9.3.0",
    "input-otp": "^1.4.2",
    "lodash": "4.17.21",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-intl": "4.3.9",
    "next-themes": "^0.4.6",
    "prettier": "3.6.2",
    "react": "19.1.0",
    "react-circle-flags": "0.0.23",
    "react-countdown-circle-timer": "3.2.1",
    "react-dom": "19.1.0",
    "react-phone-number-input": "3.4.12",
    "resend": "6.1.2",
    "sonner": "^2.0.7",
    "sort-json": "2.0.1",
    "swiper": "12.0.2",
    "tailwind-merge": "^3.3.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/lodash": "4.17.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "6.17.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
